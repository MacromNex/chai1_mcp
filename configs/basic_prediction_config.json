{
  "_description": "Configuration for basic structure prediction",
  "_source": "Extracted from examples/use_case_1_basic_structure_prediction.py",

  "model": {
    "num_trunk_recycles": 3,
    "num_diffn_timesteps": 200,
    "seed": 42,
    "use_esm_embeddings": true
  },

  "computation": {
    "device": "cuda:0",
    "fallback_device": "cpu"
  },

  "output": {
    "format": "cif",
    "include_scores": true,
    "include_metadata": true,
    "clean_output_dir": true
  },

  "validation": {
    "validate_fasta": true,
    "min_sequence_length": 1,
    "max_sequence_length": 10000
  }
}